{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "node"
    ]
  }
}
